[run]
branch = True
source = venom_core
omit = tests/*
    venom_core/nodes/protocol.py
    venom_core/agents/writer.py
    venom_core/infrastructure/onnx_runtime.py
    venom_core/perception/antenna.py

[report]
exclude_lines =
    if __name__ == "__main__":
    pragma: no cover
    if TYPE_CHECKING:
    if settings\.DEBUG
    if SETTINGS\.DEBUG
    logger\.
    logging\.
    raise NotImplementedError
    except ImportError
    @abstractmethod
    class.*\(.*Protocol.*\):
    class.*\(.*ABC.*\):
