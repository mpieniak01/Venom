{
  "intents": {
    "TIME_REQUEST": {
      "threshold": 0.85,
      "phrases": [
        "wie spaet ist es",
        "wie spat ist es",
        "wie viel uhr ist es",
        "uhrzeit",
        "aktuelle uhrzeit",
        "zeit jetzt",
        "sag mir die uhrzeit",
        "wie spaet",
        "wie spät"
      ],
      "regex": [
        ".*(uhr|uhrzeit|zeit|spaet|spat).*"
      ]
    },
    "INFRA_STATUS": {
      "threshold": 0.86,
      "phrases": [
        "status der dienste",
        "dienststatus",
        "systemstatus",
        "systemzustand",
        "zustand der dienste"
      ],
      "regex": [
        ".*(status|zustand).*(dienst|system).*"
      ]
    },
